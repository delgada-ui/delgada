{
  "name": "@delgada/core",
  "version": "0.4.0",
  "description": "A frontend web compiler for building slim UIs.",
  "author": "Hawk Ticehurst <hawkticehurst@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/delgada-ui/delgada#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/delgada-ui/delgada.git"
  },
  "bugs": {
    "url": "https://github.com/delgada-ui/delgada/issues"
  },
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release",
    "build:debug": "napi build --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "version": "napi version",
    "test": "ava"
  },
  "devDependencies": {
    "@napi-rs/cli": "^2.0.0",
    "ava": "^3.15.0"
  },
  "napi": {
    "name": "core",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  },
  "engines": {
    "node": ">= 10"
  },
  "keywords": [
    "delgada",
    "ui",
    "components",
    "web",
    "framework",
    "compiler",
    "templating"
  ]
}
